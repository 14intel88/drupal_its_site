        {
            "value": "<table border=\"1\" cellpadding=\"1\" cellspacing=\"1\" style=\"width: 777px;\">\r\n\t<tbody>\r\n\t\t<tr>\r\n\t\t\t<td style=\"width: 386px;\">\r\n\t\t\t<h1><strong>\u0412\u0438\u043f\u0443\u0441\u043a\u043d\u0438\u043a\u0438 \u043a\u0430\u0444\u0435\u0434\u0440\u0438<\/strong><\/h1>\r\n\t\t\t<\/td>\r\n\t\t\t<td style=\"width: 351px;\">\r\n\t\t\t<h1><strong>\u200b\u041a\u043e\u043c\u043f\u0430\u043d\u0456\u0457,<br \/>\r\n\t\t\t\u0432 \u044f\u043a\u0438\u0445 \u043f\u0440\u0430\u0446\u044e\u044e\u0442\u044c \u0432\u0438\u043f\u0443\u0441\u043a\u043d\u0438\u043a\u0438 \u043a\u0430\u0444\u0435\u0434\u0440\u0438<\/strong><\/h1>\r\n\t\t\t<\/td>\r\n\t\t<\/tr>\r\n\t\t<tr>\r\n\t\t\t<td style=\"width: 386px;\">\r\n\t\t\t<p><strong>\u0413\u0440\u0443\u043f\u0430 \u0422\u0406-42\u043c&nbsp;-&nbsp;\u043c\u0430\u0433\u0456\u0441\u0442\u0440\u0438&nbsp;2010\u0440.<\/strong><\/p>\r\n\r\n\t\t\t<p><strong><img alt=\"\u0422\u041842\" data-entity-type=\"file\" data-entity-uuid=\"08fd0c02-d96b-44c7-af24-b84e1f84a498\" src=\"\/sites\/default\/files\/inline-images\/%D0%A2%D0%98%2042%D0%BC_0.jpg\" \/><\/strong><\/p>\r\n\r\n\t\t\t<p><strong>\u0413\u0440\u0443\u043f\u0430 \u0422\u0406-31\u043c - \u043c\u0430\u0433\u0456\u0441\u0442\u0440\u0438 2009\u0440.<\/strong><\/p>\r\n\r\n\t\t\t<p><strong><img alt=\"\u0412\u0438\u043f\u0443\u0441\u043a 2009\" data-entity-type=\"file\" data-entity-uuid=\"c4501f79-b485-48a6-9a6d-4edf808095ac\" src=\"\/sites\/default\/files\/inline-images\/vypusk2009.jpg\" \/><\/strong><\/p>\r\n\r\n\t\t\t<p>\u0411\u0435\u0440\u0435\u0437\u043e\u0432\u0441\u044c\u043a\u0438\u0439 \u0411\u043e\u0433\u0434\u0430\u043d \u0410\u043d\u0430\u0442\u043e\u043b\u0456\u0439\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0411\u043e\u0440\u0441\u0443\u043a\u043e\u0432 \u0420\u043e\u043c\u0430\u043d \u0410\u043d\u0430\u0442\u043e\u043b\u0456\u0439\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0413\u0440\u0435\u0431\u0435\u043d\u044e\u043a \u041a\u043e\u0441\u0442\u044f\u043d\u0442\u0438\u043d \u041e\u043b\u0435\u0433\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041a\u043e\u0442 \u0422\u0435\u0442\u044f\u043d\u0430 \u041c\u0438\u043a\u043e\u043b\u0430\u0457\u0432\u043d\u0430&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041c\u0456\u0449\u0435\u043d\u043a\u043e \u041e\u043b\u0435\u0433 \u041c\u0438\u043a\u043e\u043b\u0430\u0439\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041f\u043e\u043d\u043e\u043c\u0430\u0440\u0435\u043d\u043a\u043e \u0414\u0435\u043d\u0438\u0441 \u0412\u043e\u043b\u043e\u0434\u0438\u043c\u0438\u0440\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0428\u0435\u0441\u0442\u0430\u043a \u0412\u0430\u0441\u0438\u043b\u044c \u0406\u0433\u043e\u0440\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0424\u0430\u0434\u0454\u0454\u0432\u0430 \u041a\u0430\u0442\u0435\u0440\u0438\u043d\u0430 \u0410\u043d\u0430\u0442\u043e\u043b\u0456\u0457\u0432\u043d\u0430<\/p>\r\n\r\n\t\t\t<p><strong>\u0413\u0440\u0443\u043f\u0430 \u0412\u0422-14\u043c -\u043c\u0430\u0433\u0456\u0441\u0442\u0440\u0438 2007\u0440.<\/strong><\/p>\r\n\r\n\t\t\t<p><strong><img alt=\"\u0412\u0438\u043f\u0443\u0441\u043a 07\" data-entity-type=\"file\" data-entity-uuid=\"46f00faf-b481-4230-aace-581994386e90\" src=\"\/sites\/default\/files\/inline-images\/vipusk07.jpg\" \/><\/strong><\/p>\r\n\r\n\t\t\t<p><strong>\u0413\u0440\u0443\u043f\u0430 \u0422\u0406-21-\u0441\u043f\u0435\u0446\u0456\u0430\u043b\u0456\u0441\u0442\u0438 2008\u0440.<\/strong><\/p>\r\n\r\n\t\t\t<p><strong><img alt=\"\u0412\u0438\u043f\u0443\u0441\u043a 08\" data-entity-type=\"file\" data-entity-uuid=\"cd036d21-04a1-4b66-9537-bec92dffd189\" src=\"\/sites\/default\/files\/inline-images\/special.jpg\" \/><\/strong><\/p>\r\n\r\n\t\t\t<p>\u0412\u0430\u0441\u0438\u043b\u0435\u043d\u043a\u043e \u0414\u0435\u043d\u0438\u0441 \u041c\u0438\u043a\u043e\u043b\u0430\u0439\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0412\u0435\u0440\u0433\u0435\u043b\u0435\u0441 \u041e\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440 \u041e\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0414\u0435\u0440\u0435\u0432\u2019\u044f\u043d\u0441\u044c\u043a\u0438\u0439&nbsp; \u0410\u043d\u0434\u0440\u0456\u0439&nbsp; \u0406\u0433\u043e\u0440\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0406\u0432\u043b\u0454\u0432 \u042e\u0440\u0456\u0439&nbsp; \u0412\u0430\u0441\u0438\u043b\u044c\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041c\u0430\u0437\u043a\u043e \u0410\u043d\u0442\u043e\u043d \u041e\u043b\u0435\u043a\u0441\u0456\u0439\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041e\u043b\u0456\u0439\u043d\u0438\u043a \u041e\u043b\u0435\u0433 \u041f\u0430\u0432\u043b\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041f\u0435\u0440\u0435\u043f\u044c\u043e\u043b\u043e\u0432&nbsp;&nbsp; \u041a\u0438\u0440\u0438\u043b\u043e&nbsp; \u041e\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041f\u0435\u0442\u0440\u043e\u0432\u0441\u044c\u043a\u0438\u0439&nbsp; \u041e\u043b\u0435\u043a\u0441\u0456\u0439&nbsp; \u0412\u0456\u0442\u0430\u043b\u0456\u0439\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u041f\u0440\u0438\u0439\u043c\u0430\u043a&nbsp; \u0414\u043c\u0438\u0442\u0440\u043e \u0412\u0430\u0441\u0438\u043b\u044c\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0420\u0435\u0437\u043d\u0438\u043a \u0421\u0435\u0440\u0433\u0456\u0439&nbsp; \u041c\u0438\u043a\u043e\u043b\u0430\u0439\u043e\u0432\u0438\u0447&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; \u0421\u0435\u0440\u0434\u044e